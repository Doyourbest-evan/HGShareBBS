<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="IndexConfig" type="EsData.Configs.IndexConfigSection, EsData.Configs" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <!--数据库-->
  <connectionStrings configSource="Config\ConnectionString.config" />
  <!--索引配置-->
  <IndexConfig configSource="Config\IndexConfig.config" />
  <!--log4net-->
  <log4net configSource="Config\log4net.config" />
  <appSettings>
    <!--es服务-->
    <add key="Nodes" value="localhost:9200"/>

